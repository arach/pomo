{
  "name": "@arach/dev-toolbar-react",
  "version": "1.0.0",
  "description": "A beautiful, minimal dev toolbar for React applications",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.tsx --format cjs,esm --dts --external react",
    "dev": "tsup src/index.tsx --format cjs,esm --dts --external react --watch",
    "lint": "eslint src",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "dev-tools",
    "toolbar",
    "debugging",
    "development"
  ],
  "author": "arach",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "lucide-react": "^0.263.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.5.0",
    "typescript": "^5.3.3"
  }
}