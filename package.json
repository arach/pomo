{
  "name": "pomo",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:web": "tsc && vite build --mode production",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "dev:port": "node dev-port.js",
    "tauri:build": "tauri build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "dev:compare": "vite & sleep 2 && open 'http://localhost:1421/?split=true'",
    "dev:neon": "vite & sleep 2 && open 'http://localhost:1421/?watchface=neon&version=v1' && open 'http://localhost:1421/?watchface=neon&version=v2'",
    "dev:v2": "vite & sleep 2 && open 'http://localhost:1421/?version=v2'",
    "deploy:prepare": "npm run build:web",
    "deploy": "npm run deploy:prepare && echo 'Ready to deploy dist/ to pomo.arach.dev'"
  },
  "dependencies": {
    "@arach/devbar": "link:/Users/arach/dev/devbar",
